    "n = int(input(\"Enter any No. to find the sum of Digits: \"))\n",
    "print(\"n =\" , n )\n",
    "print(getSum(n))\n",
    "\n",
    "def getSum(n):\n",
    "\tsum = 0\n",
    "\twhile (n != 0):\n",
    "\t\tsum = sum + (n % 10)\n",
    "\t\tn = int(n/10)\n",
    "\treturn sum\n"
