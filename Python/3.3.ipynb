    "def rev_num(n):\n",
    "    reverse = 0\n",
    "    while n > 0: \n",
    "        remainder = n % 10\n",
    "        reverse = (reverse * 10) + remainder \n",
    "        n = n // 10\n",
    "\n",
    "    print(\"The reverse of the number is:\", reverse) \n",
    "\n",
    "num = int(input(\"Enter a number: \")) ;    \n",
    "print(\"num =\" ,num)\n",
    "rev_num(num)\n"
   ]
  }
