   "source": [
    "n = int(input(\"Enter a number to check whether is Armstrong or Not : \"))\n",
    "print(\"n = \",n)\n",
    "sum = 0\n",
    "l = len(str(n))\n",
    "temp = n\n",
    "while temp > 0:\n",
    "   digit = temp % 10\n",
    "   sum +=  digit ** l\n",
    "   temp //= 10\n",
    "\n",
    "if n == sum:\n",
    "   print(\"Armstrong number\")\n",
    "else:\n",
    "   print(\"Not an Armstrong number\")"
   ]
  }
